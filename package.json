{
  "name": "@jacraig/woodchuck",
  "version": "0.1.2",
  "description": "A simple logging library.",
  "main": "lib/Logger.cjs.js",
  "module": "lib/Logger.esm.js",
  "browser": "lib/Logger.umd.min.js",
  "types": "lib/declarations/Logger.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/Logger.esm.js",
      "require": "./lib/Logger.cjs.js"
    },
    "./Logger.umd.js": "./lib/Logger.umd.js",
    "./Logger.umd.min.js": "./lib/Logger.umd.min.js"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "",
    "pack": "npm pack",
    "prepublish": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaCraig/Woodchuck.git"
  },
  "keywords": [
    "logging",
    "logger",
    "log",
    "utility",
    "utilities"
  ],
  "author": "James Craig",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JaCraig/Woodchuck/issues"
  },
  "homepage": "https://github.com/JaCraig/Woodchuck#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.4",
    "semantic-release": "^23.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ]
  }
}
